{
  "kind": "collectionType",
  "collectionName": "products",
  "info": {
    "singularName": "product",
    "pluralName": "products",
    "displayName": "Product",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Title": {
      "type": "string",
      "required": true
    },
    "Slug": {
      "type": "uid",
      "required": true
    },
    "Description": {
      "type": "text"
    },
    "images": {
      "type": "media",
      "multiple": false,
      "required": true,
      "allowedTypes": [
        "images"
      ]
    },
    "collections": {
      "type": "enumeration",
      "enum": [
        "bags",
        "shirts",
        "pents"
      ],
      "default": "bags"
    },
    "colour": {
      "type": "enumeration",
      "enum": [
        "Red",
        "Black",
        "White",
        "Brown"
      ],
      "default": "Red"
    },
    "Price": {
      "type": "integer"
    },
    "Url": {
      "type": "string",
      "required": true
    }
  }
}
